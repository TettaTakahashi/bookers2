      <h2>Users</h2>
        
      <table>
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
          </tr>
        </thead>
        
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td></td><!--ä»®-->
              <td><%= user.name %></td>
              <td><%= link_to %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
